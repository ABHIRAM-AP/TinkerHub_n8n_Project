    {
      "parameters": {
        "jsCode": "// Get the search results from YouTube\nconst searchResults = $input.first().json;\n\n// Get the original song data from Loop Over Items\nconst songData = $('Loop Over Songs').item.json;\n\n// Check if we have valid results\nif (searchResults.items && searchResults.items.length > 0) {\n  // Extract video information from first result\n  const firstResult = searchResults.items[0];\n  const videoId = firstResult.id.videoId;\n  const videoTitle = firstResult.snippet.title;\n  const channelTitle = firstResult.snippet.channelTitle;\n  \n  // Return the data needed for adding to playlist\n  return [{\n    json: {\n      videoId: videoId,\n      videoTitle: videoTitle,\n      channelTitle: channelTitle,\n      songTitle: songData.songTitle,\n      artist: songData.artist,\n      playlistId: songData.playlistId,\n      found: true\n    }\n  }];\n  \n} else {\n  // No results found for this song\n  return [{\n    json: {\n      songTitle: songData.songTitle,\n      artist: songData.artist,\n      searchQuery: songData.searchQuery,\n      found: false,\n      error: 'No video found on YouTube'\n    }\n  }];\n}"
      },
      "type": "n8n-nodes-base.code",
